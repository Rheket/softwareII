<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.reportsViewController">
   <children>
      <HBox prefHeight="400.0" prefWidth="600.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="244.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#mainMenu" text="Main Menu" />
                  <Label text="Select a report to run" />
                  <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#appointmentsPerMonth" prefHeight="50.0" prefWidth="180.0" text="Appointments Per Month" />
                  <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#consultantSchedule" prefHeight="50.0" prefWidth="180.0" text="Consultant Schedule" />
                  <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#numberOfAppointmentsCustomer" prefHeight="50.0" prefWidth="180.0" text="Appointments Per Customer" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
